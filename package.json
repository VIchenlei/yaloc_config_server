{
  "name": "loc-server",
  "version": "3.0.0",
  "description": "Indoor Locating Server",
  "main": "index.js",
  "author": "HANK WANG",
  "license": "MIT",
  "scripts": {
    "start": "babel-node ./index.js",
    "dev": "babel-node ./index.js",
    "pro": "node ./index-pro.js",
    "build": "babel ./src --out-dir ./bin",
    "debug-socket": "DEBUG=socket.io* babel-node ./index.js",
    "debug": "babel-node --inspect --inspect=5858 ./index.js",
    "format": "standard --fix ./src/**/*.js"
  },
  "keywords": [
    "Indoor",
    "Locating",
    "Server"
  ],
  "dependencies": {
    "ejsexcel": "^3.5.2",
    "express": "^4.15.4",
    "express-session": "^1.15.5",
    "express-socket.io-session": "^1.3.2",
    "fast-csv": "^2.4.0",
    "mysql": "^2.14.1",
    "pdfmake": "^0.1.32",
    "session-file-store": "^1.1.2",
    "socket.io": "^2.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "standard": "^10.0.3"
  }
}
